{
  "name": "reactn",
  "version": "1.0.0",
  "private": true,
  "frontend": {
    "name": "Reactn: A React and Socket.io based chat app by Ben Halverson",
    "javascripts": "src/components",
    "stylesheets": "src/scss"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "socket.io": "2.1.0",
    "uuid": "3.2.1",
    "uuidv4": "1.0.0"
  },
  "scripts": {
    "start": "react-scripts start && server",
    "build": "react-scripts build",
    "server": "nodemon src/server/server.js",
    "test": "react-scripts test --env=jsdom",
    "styles": "sass --watch --style compressed src/scss/app.scss:src/css/app.css",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "homepage": "http://github.com/benhalverson/react-chatroom-ui",
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-react": "7.7.0",
    "nodemon": "^1.17.5"
  }
}
